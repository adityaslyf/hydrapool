{
  "framework": null,
  "buildCommand": "pnpm run build:vercel",
  "installCommand": "pnpm install",
  "functions": {
    "app/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "SKIP_ENV_VALIDATION": "1",
    "DISABLE_ESLINT_PLUGIN": "true",
    "ESLINT_NO_DEV_ERRORS": "true"
  },
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1",
      "SKIP_ENV_VALIDATION": "1",
      "DISABLE_ESLINT_PLUGIN": "true",
      "ESLINT_NO_DEV_ERRORS": "true"
    }
  }
}
